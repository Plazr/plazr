<h1>Users</h1>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.first_name %>
      <%= user.email %>
      <%= link_to 'Show', root_user_path(user) %>
      <%#= link_to 'Editar', edit_root_user_path(user) %>
      <%= link_to "Delete", root_user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %>
    </li>
  <% end %>
</ul>

<%= link_to "Back to Home", plazrrs_path %>